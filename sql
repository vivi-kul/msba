USE 

SELECT
  SELECT DISTINCT
  TOP 5/BOTTOM
  TOP 5 PERCENT
  TOP 5 WITH TIES
  GETDATE()
  LEFT
  
  AS
FROM
WHERE 
  IN
  BETWEEN
  LIKE
ORDER BY
  ASC/DESC
  FETCH FIRST|NEXT ONLY
  OFFSET FIRST 5 ROWS

GETDATE()
LEFT

# Comparison Operators
=
>
<
<=
>=
<>

# Logical operators
NOT
AND
OR
IN 
BETWEEN
LIKE

# Wildcard symbols
%
_
[]
[ - ]
[ ^ ]

# Null
IS [NOT] NULL
